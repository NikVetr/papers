  keywords = {Animals,{Animals, Wild},Antigens,Cattle,{Contraception, Immunologic},Deer,Egg Proteins,Equidae,Female,Fertility,Gonadotropin-Releasing Hormone,Male,Membrane Glycoproteins,Population Control,{Receptors, Cell Surface},Species Specificity,Swine,Treatment Outcome,Vaccination,{Vaccines, Contraceptive},{Vaccines, Synthetic},Zona Pellucida,Zona Pellucida Glycoproteins},
