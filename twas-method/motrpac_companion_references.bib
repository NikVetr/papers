@article{knutson_Anticipation_1998,
  title = {Anticipation of Play Elicits High-Frequency Ultrasonic Vocalizations in Young Rats},
  author = {Knutson, Brian and Burgdorf, Jeffrey and Panksepp, Jaak},
  year = {1998},
  journal = {Journal of Comparative Psychology},
