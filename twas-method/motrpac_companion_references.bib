@article{pyzyna_ContraPest_2018,
  title = {{{ContraPest}}\textregistered, a {{New Tool}} for {{Rodent Control}}},
  author = {Pyzyna, Brandy R. and Trulove, Nicholas F. and Mansfield, Clayton H. and McMillan, Ryan A. and Ray, Courtney N. and Mayer, Loretta P.},
  year = {2018},
  journal = {Proceedings of the Vertebrate Pest Conference},
  volume = {28},
  number = {28},
  issn = {0507-6773},
  doi = {10.5070/V42811054},
  urldate = {2023-02-13},
  abstract = {Lethal rodenticides and other lethal tools of managing commensal rodent populations long-term are not sustainable due to population rebounds and increasing resistance to rodenticides. The use of integrated pest management (IPM) programs are more prevalent due to consumer desire to decrease rodenticide use and utilize environmentally friendly, humane methods. IPM plans often require multiple tools to control an infestation, such as physical, biological and chemical measures. Here, we propose that rodent population management would benefit from a new tool aimed at targeting the biological source of overabundance: reproduction. SenesTech, Inc. (Flagstaff, AZ USA) has developed ContraPest\textregistered, a liquid bait that limits the reproductive capacity of both male and female wild Norway and Roof rats. The two active ingredients, 4-vinylcyclohexene diepoxide (VCD) and triptolide, deplete all stages of follicles in the female and disrupt spermatogenesis in the male. Laboratory and field studies reveal that ContraPest\textregistered{} is palatable and repeatedly consumed by rats even when provided with ad libitum food and water. Studies involving laboratory and wild caught rats have demonstrated a 93 - 100\% reduction in litter sizes of rats treated with ContraPest\textregistered{} compared to control rats. ContraPest\textregistered{} was tested on free ranging rat populations in agricultural and urban settings. Rat populations on protein production farms decreased by an average of 46\% following 100 days of treatment with ContraPest\textregistered. In a complex urban environment, where property boundaries limit access to populations and foraging areas, ContraPest\textregistered{} reduced the seasonal population peak by 67\% after 133 days of baiting. These studies, combined with all of our field studies and population reduction models, demonstrate that ContraPest\textregistered{} is a highly effective rodent contraceptive bait in a variety of environments. We strongly believe that adding/implementing fertility management via ContraPest\textregistered{} to an IPM program would enhance long-term rodent population control in rural, urban, and agricultural environments.},
  langid = {english},
  file = {/Users/nikgvetr/Zotero/storage/QF2LHF9E/Pyzyna et al. - 2018 - ContraPest®, a New Tool for Rodent Control.pdf}
}

