  author = {Lepeule, Johanna and Baccarelli, Andrea and Tarantini, Letizia and Motta, Valeria and Cantone, Laura and Litonjua, Augusto A. and Sparrow, David and Vokonas, Pantel S. and Schwartz, Joel},
  year = {2012},
